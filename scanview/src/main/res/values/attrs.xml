<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScanView">
        <attr name="scanLineDrawable" format="reference"/>
        <attr name="scanFrameDrawable" format="reference"/>

        <attr name="scanLineHeight" format="dimension"/>

        <attr name="duration" format="integer"/>

        <attr name="frameWidth" format="dimension"/>
        <attr name="frameHeight" format="dimension"/>
        <attr name="framePadding" format="dimension"/>
        <attr name="framePaddingLeft" format="dimension"/>
        <attr name="framePaddingRight" format="dimension"/>
        <attr name="framePaddingTop" format="dimension"/>
        <attr name="framePaddingBottom" format="dimension"/>

        <attr name="lineWidth" format="dimension"/>
        <attr name="lineHeight" format="float"/>
        <attr name="linePadding" format="dimension"/>
        <attr name="linePaddingLeft" format="dimension"/>
        <attr name="linePaddingRight" format="dimension"/>
        <attr name="linePaddingTop" format="dimension"/>
        <attr name="linePaddingBottom" format="dimension"/>

        <attr name="dim_border" format="color"/>
    </declare-styleable>
</resources>
